#include <iostream>
#include <queue>
using namespace std;
int map[51][51]={ 0, };
int dx[]={-1,0,1,0};
int dy[]={0,-1,0,1};
bool visited[51][51];
int cnt=0;

void BFS(int x,int y){
  queue<pair<int ,int>> q;
  q.push(make_pair(x,y));
  visited[x][y]=true;
  while(!q.empty()){
    x=q.front().first;
    y=q.front().second;
    q.pop();
    
}
